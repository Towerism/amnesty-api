{
  "name": "amnesty-api",
  "version": "1.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/start",
    "dev": "set NODE_ENV=development&& nodemon --inspect=3000 ./bin/start",
    "db:seed": "sequelize db:seed:all",
    "db:migrate": "sequelize db:migrate",
    "db:init": "npm run db:migrate && npm run db:seed",
    "test": "set NODE_ENV=test&& mocha --recursive --require aurelia-polyfills ./test/*Test.js --compilers js:babel-core/register"
  },
  "dependencies": {
    "aurelia-dependency-injection": "^1.3.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.16.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.0.0",
    "enumify": "^1.0.4",
    "events": "^1.1.1",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "generator-express-es6": "^0.4.0",
    "jsonwebtoken": "^7.4.3",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.3.0",
    "sequelize": "^4.7.5",
    "sequelize-cli": "^2.8.0",
    "sequelize-fixtures": "^0.6.0",
    "serve-favicon": "^2.4.3"
  },
  "devDependencies": {
    "aurelia-polyfills": "^1.2.2",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.2.1",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "sqlite3": "^3.1.9"
  },
  "description": "Api for Amnesty International Blog",
  "author": "Martin Fracker <martin.frackerjr@gmail.com>",
  "license": "GNU GPL v3"
}
